	<queryString>
		<![CDATA[SELECT MaHD,MaTD,ct.MaMA,TenMA,MaPL, ma.GiaTien AS ChiPhi,ct.ChiPhiPhatSinh,ThuTu,IIF(ct.SoLuong = -1,'CXD',CAST(ct.SoLuong AS varchar)) AS SoLuong ,
		IIF ( ct.SoLuong * ma.GiaTien < 0, 'CXD',CAST(ct.SoLuong * ma.GiaTien AS varchar) ) AS TongChiPhi,
		GhiChu,HinhAnh FROM ChiTietDatMon ct
		INNER JOIN MonAn ma ON ct.MaMA = ma.MaMA 
		WHERE MaHD = $P{MaHD} and MaTD = $P{MaTD_Phu} order by thutu asc]]>
	</queryString>
